{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "cfng-expedientefiscalelectronico-caso-frontend": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss",
                    "skipTests": true
                },
              "@schematics/angular:service": {
                "skipTests": true
              }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "sourceMap": true,
                        "outputPath": "dist/cfng-expedientefiscalelectronico-caso-frontend",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "allowedCommonJsDependencies": [
                            "html2canvas",
                            "exceljs",
                            "file-saver",
                            "canvg",
                            "core-js",
                            "raf",
                            "rgbcolor",
                            "inputmask",
                            "jspdf-autotable",
                            "leaflet",
                            "dayjs"
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets/i18n",
                            "src/web.config",
                            "src/assets",
                            {
                                "glob": "pdf.worker.min.js",
                                "input": "node_modules/pdfjs-dist/build/",
                                "output": "/assets/"
                            },
                            {
                                "input": "./node_modules/ngx-extended-pdf-viewer/assets/",
                                "glob": "**/{locale,es-ES}/?*.properties",
                                "ignore": [
                                    "**/additional-locale/*"
                                ],
                                "output": "./assets/"
                            },
                            {
                                "glob": "**/*",
                                "input": "node_modules/ngx-extended-pdf-viewer/assets/",
                                "output": "/assets/"
                            },
                            {
                                "glob": "**/*",
                                "input": "./node_modules/leaflet/dist/images",
                                "output": "assets/"
                            }
                        ],
                        "styles": [
                            "src/styles.scss",
                            "node_modules/shepherd.js/dist/css/shepherd.css"
                        ],
                        "stylePreprocessorOptions":{
                          "includePaths": [
                            "src/app"
                          ]
                        },
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "5mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "5mb",
                                    "maximumError": "5mb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "preprod": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.preprod.ts"
                                }
                            ]
                        },
                        "qa": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.qa.ts"
                                }
                            ]
                        },
                        "dev": {
                            "optimization": false,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.dev.ts"
                                }
                            ]
                        },
                        "local": {
                            "optimization": false
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "cfng-expedientefiscalelectronico-caso-frontend:build:production"
                        },
                        "preprod": {
                            "buildTarget": "cfng-expedientefiscalelectronico-caso-frontend:build:preprod"
                        },
                        "qa": {
                            "buildTarget": "cfng-expedientefiscalelectronico-caso-frontend:build:qa"
                        },
                        "dev": {
                            "buildTarget": "cfng-expedientefiscalelectronico-caso-frontend:build:dev"
                        },
                        "local": {
                            "buildTarget": "cfng-expedientefiscalelectronico-caso-frontend:build:local"
                        }
                    },
                    "defaultConfiguration": "local"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "cfng-expedientefiscalelectronico-caso-frontend:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "tsconfig.spec.json",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "public"
                            },
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": "62e1f0be-c0ea-4322-b5e6-0518296c24de"
    }
}
